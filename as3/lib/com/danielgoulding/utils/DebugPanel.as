package com.danielgoulding.utils {	import flash.display.DisplayObjectContainer;	import flash.text.TextField;			/**	 * @author Dan	 */	public class DebugPanel {								public static function trace(object:DisplayObjectContainer, message:String):void{			var tf:TextField = new TextField();			tf.width = 500;			tf.height = 200;			tf.multiline = true;			tf.wordWrap = true;			object.addChild(tf);			tf.text = message;		}					}}