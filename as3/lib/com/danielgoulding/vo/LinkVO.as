package com.danielgoulding.vo {	/**	 * @author danielgoulding	 */	public class LinkVO {		//----------------------------------------------------------------------		//  VARIABLES:		//----------------------------------------------------------------------		private var _title						: String;		private var _url						: String;		private var _target						: String;		//----------------------------------------------------------------------		//  CONSTRUCTOR:		//----------------------------------------------------------------------		public function LinkVO( params:Object = null ) {			if ( params )				setProperties( params );		}		//----------------------------------------------------------------------		//  PUBLIC API:		//----------------------------------------------------------------------		public function setProperties( params:Object ):void{			if ( params['title'] != null )				title = params['title'];			if ( params['url'] != null )				url = params['url'];			if ( params['target'] != null )				target = params['target'];		}		public function toString():String{			return "<LinkVO: title='" + title + "' url='" + url + "' target='" + target + "' />";		}		//----------------------------------------------------------------------		//  GET / SET:		//----------------------------------------------------------------------		public function get title() : String {			return _title;		}		public function set title(title : String) : void {			_title = title;		}		public function get url() : String {			return _url;		}		public function set url(url : String) : void {			_url = url;		}		public function get target() : String {			return _target;		}		public function set target(target : String) : void {			_target = target;		}	}}